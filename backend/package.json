{
  "name": "restaurante",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "front": "yarn --cwd ../front run start",
    "start": "concurrently \"yarn dev\" \"yarn front\""
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
